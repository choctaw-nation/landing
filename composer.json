{
	"name": "cno/choctaw-landing-theme",
	"description": "The theme for choctawlanding.com temp",
	"type": "project",
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0.0"
	},
	"scripts": {
		"lint:php": "phpcs  ./wp-content/themes/bootscore-5 --standard=Wordpress --extensions=php",
		"fix:php": "phpcbf --standard=Wordpress --extensions=php ./wp-content/themes/bootscore-5"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"authors": [
		{
			"name": "Choctaw Landing"
		}
	]
}
